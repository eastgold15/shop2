{
  "name": "@gina/monorepo",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.4",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "check": "npx ultracite check",
    "check-type": "turbo run check-type",
    "clean": "bunx turbo@latest run clean  && rm -rf dist .turbo",
    "prepare": "husky",
    "generate-types": "bunx openapi-typescript apps/b2badmin/src/types/temp-schema.json  -o ./apps/b2badmin/src/types/api-schema.d.ts",
    "fix": "npx ultracite fix",
    "docker:compose": "docker compose up -d",
    "gen:ai-docs": "bun run  scripts/gen-ai-docs.ts",
    "gen:final": "bun run packages/contract/scripts/gen-all-final.ts",
    "postgen:final": "bun run fix",
    "ast:Gen": "bun run scripts/gen-ast.ts",
    "pipelineGen": "bun run script/gen.ts",
    "postpipelineGen": "bun run fix"
  },
  "engines": {
    "bun": ">=1.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@types/fs-extra": "^11.0.4",
    "@types/pg": "^8.16.0",
    "fs-extra": "^11.3.3",
    "husky": "^9.1.7",
    "lefthook": "^2.0.11",
    "lint-staged": "^16.2.7",
    "openapi-typescript": "^7.10.1",
    "ultracite": "6.4.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "@sinclair/typebox": "^0.34.41",
    "pg": "^8.16.3",
    "ts-morph": "^27.0.2",
    "turbo": "^2.6.3",
    "typescript": "^5.9.3"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "@types/bun": "^1.3.4",
      "elysia": "1.4.18",
      "typescript": "^5.9.3",
      "@biomejs/biome": "2.3.10",
      "better-auth": "^1.3.34",
      "drizzle-orm": "^1.0.0-beta.2-58a4521",
      "@types/pg": "^8.16.0",
      "drizzle-kit": "^1.0.0-beta.2-58a4521",
      "drizzle-typebox": "^1.0.0-beta.2-58a4521",
      "tsx": "^4.20.6",
      "logixlysia": "^5.3.0"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "bun x ultracite fix",
      "bun x ultracite fix"
    ]
  }
}
